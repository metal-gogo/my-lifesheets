{
	"app": {
		"name": "Work time tracker",
		"slug": "work-time-tracker",
		"entry_name": "Hours worked",
		"description": "Hours worked tracker",
		"private": false,
		"icon": "ðŸ˜€",
		"color": "#ee3f3f",
		"storageType": "",
		"storage": "",
		"charts_placement": "",
		"entries_name": "Hours workeds"
	},
	"temporal": {
		"times": "yes",
		"ranges": "yes"
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				]
			},
			"name": "date"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " at ",
				"type": "time",
				"default": "[time($startup, 'hours')]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(date, time)"
			},
			"name": "date_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " to ",
				"type": "time",
				"default": "",
				"visible": "[time]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "end_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"prefix": " on ",
				"visible": "[if(time and end_time, if(end_date = date, 'edit', true), false)]",
				"default": "[if(end_time, if(end_time < time, date(date + 1 * days()), date))]",
				"min_date": "[date]"
			},
			"name": "end_date"
		},
		{
			"type": "spacer",
			"scope": "entry",
			"settings": {}
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"visible": "[end_date and (time and end_time)]",
				"prefix": "(",
				"suffix": ")",
				"expression": "duration(duration, 2)"
			},
			"name": "duration_readable"
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"expression": "if(Project = \"Personal\", 0, 50)",
				"visible": "false"
			},
			"name": "hourly_rate"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "options",
				"options": "Personal, Breeze, Synergy, Quest",
				"default": "[Personal]",
				"appearance": "line"
			},
			"name": "Project"
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"expression": "hours(duration)*hourly_rate",
				"prefix": "Money earned $",
				"appearance": "line",
				"visible": "[if(Project != \"Personal\")]"
			},
			"name": "money_earned"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(end_date, end_time)"
			},
			"name": "end_date_time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "end_date_time - date_time"
			},
			"name": "duration"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "text",
				"appearance": "line",
				"multiline": true
			},
			"name": "notes"
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"expression": "hours(duration)",
				"visible": "false"
			},
			"name": "duration_in_hours"
		}
	],
	"root": [
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"label": "âž• Add Hours worked",
				"target": "add(entry)",
				"target_preset": "new",
				"appearance": "",
				"set_pairs": [],
				"filter_pairs": []
			}
		},
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"expression": "sum(money_earned)",
				"prefix": "Money earned to date: $"
			}
		}
	],
	"charts": [
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"aggregate": "sum",
				"aggregate_of": "money_earned",
				"group_by": "Project"
			}
		},
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"aggregate": "sum",
				"aggregate_of": "duration",
				"group_by": "Project",
				"type": "table"
			}
		}
	]
}