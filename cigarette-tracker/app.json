{
	"app": {
		"name": "Cigarette Tracker",
		"slug": "cigarette-tracker",
		"entry_name": "Cigarrette smoked",
		"description": "Cigarrette smoked tracker",
		"private": false,
		"icon": "ðŸš¬",
		"color": "#969696",
		"storageType": "",
		"storage": "",
		"charts_placement": "",
		"entries_name": "Cigarrettes smoked"
	},
	"temporal": {
		"times": "yes",
		"ranges": ""
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				]
			},
			"name": "date"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " at ",
				"type": "time",
				"default": "[time($startup, 'hours')]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "datetime(date, time)"
			},
			"name": "date_time"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"default": "[1]",
				"number_style": "bar",
				"bar_style": "discrete",
				"max": 10
			},
			"name": "cigarretes_smoked"
		}
	],
	"root": [
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"visible": "edit",
				"label": "âž• Add Cigarrete smoked",
				"target": "add(entry)",
				"target_preset": "new",
				"appearance": "",
				"set_pairs": [],
				"filter_pairs": []
			}
		},
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"expression": "duration($now - last(date_time))",
				"prefix": "Time since last cigarrette:",
				"formatting": [],
				"visible": "",
				"appearance": "line",
				"text_color": "#22252a"
			},
			"name": "time_since_last_cigarrete"
		},
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"target_preset": "new",
				"target": "add(entry)",
				"set_pairs": [
					{
						"name": "cigarretes_smoked",
						"value": "2"
					}
				],
				"filter_pairs": [],
				"label": "+ Add two cigarrettes"
			}
		}
	],
	"charts": [
		{
			"type": "chart",
			"scope": "charts",
			"settings": {
				"type": "bar",
				"orientation": "horizontal",
				"group_by": "day",
				"aggregate_of": "cigarretes_smoked",
				"aggregate": "sum"
			}
		}
	]
}